<UserControl x:Class="SectionsEC.Views.RightPanel"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:SectionsEC.Views"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <DockPanel>
        <GroupBox Header="Results" DockPanel.Dock="Top">
            <TextBlock>
                <Run Text="Section capacity: "/>
                <Run Text=" 500 " FontWeight="DemiBold" TextDecorations="Underline" Foreground="Blue"/>
                <Run Text=" [kNm]"/>
            </TextBlock>
        </GroupBox>
        
    </DockPanel>
</UserControl>
